{
    "connections": [
        {
            "port": {
                "$filter": "port",
                "$default": 8081,
                "port2": 8082
            },
            "routes": {
                "timeout": {
                    "server": 10000
                }
            },
            "load": {
                "maxHeapUsedBytes": 1073741824,
                "maxRssBytes": 2147483648,
                "maxEventLoopDelay": 60000
            },
            "labels": [
                "api",
                "http"
            ]
        }
    ],
    "server": {
        "load": {
            "sampleInterval": 1000
        }
    },
    "plugins": {
        "$filter": "env",
        "dev": {
            "good": {
                "opsInterval": 5000,
                "requestHeaders": true,
                "reporters": [
                {
                    "reporter": "good-console",
                    "events": { "log": "*", "error": "*" }
                },
                {
                    "reporter": "good-file",
                    "events": { "response": "*" },
                    "config": "/home/lloyd/ficion/log/tacklebox/response.log"
                },
                {
                    "reporter": "good-file",
                    "events": { "ops": "*" },
                    "config": "/home/lloyd/ficion/log/tacklebox/ops.log"
                },
                {
                    "reporter": "good-file",
                    "events": { "log": "*" },
                    "config": "/home/lloyd/ficion/log/tacklebox/log.log"
                },
                {
                    "reporter": "good-file",
                    "events": { "error": "*" },
                    "config": "/home/lloyd/ficion/log/tacklebox/error.log"
                }]
            },
            "tacklebox": {
                "job": {
                    "dirPath": "/home/lloyd/ficion/job",
                    "workspace": "workspace"
                },
                "reel": {
                    "dirPath": "/home/lloyd/ficion/reel"
                },
                "user": {
                    "dirPath": "/home/lloyd/ficion/user"
                },
                "github": {
                    "token": "token"
                }
            },
            "lout": {}
        },
        "$default": {
            "good": {
                "opsInterval": 5000,
                "requestHeaders": true,
                "reporters": [
                {
                    "reporter": "good-file",
                    "events": { "response": "*" },
                    "config": "/app/ficion/log/tacklebox/response.log"
                },
                {
                    "reporter": "good-file",
                    "events": { "ops": "*" },
                    "config": "/app/ficion/log/tacklebox/ops.log"
                },
                {
                    "reporter": "good-file",
                    "events": { "log": "*" },
                    "config": "/app/ficion/log/tacklebox/log.log"
                },
                {
                    "reporter": "good-file",
                    "events": { "error": "*" },
                    "config": "/app/ficion/log/tacklebox/error.log"
                }]
            },
            "tacklebox": {
                "job": {
                    "dirPath": "/app/ficion/job",
                    "workspace": "workspace"
                },
                "reel": {
                    "dirPath": "/app/ficion/reel"
                },
                "user": {
                    "dirPath": "/app/ficion/user"
                },
                "github": {
                    "token": "token"
                }

            }
        }
    }
}
